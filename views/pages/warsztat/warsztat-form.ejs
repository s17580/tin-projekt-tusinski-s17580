        <li><a href ="adres.html">Adres</a></li>
        <li><a href ="../index.html">Wyloguj</a></li>
        
    </ul>
</nav>
<main>
    <h2>Nowy warsztat</h2>
    <form class="form" onsubmit="return validateForm();" novalidate onsubmit="return validateForm();">

        
        <label for="nazwa_warsztat">Nazwa warsztatu:<abbr title="required" aria-label="required">*</abbr></label>
        <input type="text" name="nazwa_warsztat" id="nazwa_warsztat" placeholder="2-30 znaków" />
        <span id="errorNazwa_Warsztat" class="errors-text"></span>
       
        
        <label for="telefon">Telefon:<abbr title="required" aria-label="required">*</abbr></label>
        <input type="text" name="telefon" id="telefon" placeholder="3-25 znaków" />
        <span id="errorTelefon" class="errors-text"></span>
       

        <label for="email">E-mail:<abbr title="required" aria-label="required">*</abbr></label>
        <input type="email" name="email" id="email" placeholder="np. nazwa@domena.pl" />
        <span id="errorEmail" class="errors-text"></span>
        
        
        <div class = "form-buttons" >
            <p id="errorsSummary" class="errors-text"></p>
            <input class = "form-button-submit" type="submit" value="Dodaj" />
            <a class = "form-button-cancel" href="warsztat-list.html">Anuluj</a>    
        </div>  
        
    </form>
    
</main>
<footer>
    Karol,Tusiński, s17580
</footer>
    
